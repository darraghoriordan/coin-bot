{
    "name": "code-bot-be",
    "version": "1.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "up": "mkdir -p ./.docker-compose/.persist/postgres && docker compose up -d",
        "down": "docker compose down",
        "prebuild": "rimraf dist",
        "build": "nest build --tsc --path tsconfig.build.json",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "npx eslint -c .eslintrc.js --ext .ts src/**/*.ts --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --runInBand --config ./test/jest-e2e.json",
        "pretypeorm": "(rm ormconfig.json || true) && node node_modules/@darraghor/nest-backend-libs/dist/database-postgres/write-typeorm-config.js",
        "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
        "typeorm:migration:generate": "npm run typeorm -- migration:generate -n",
        "typeorm:migration:run": "npm run typeorm -- migration:run",
        "typeorm:migration:revert": "npm run typeorm -- migration:revert",
        "generate": "sh ./node_modules/@darraghor/nest-backend-libs/dist/open-api-generation/generate.sh"
    },
    "dependencies": {
        "@darraghor/nest-backend-libs": "1.17.1",
        "@nestjs/cli": "7.6.0",
        "@nestjs/common": "7.6.18",
        "@nestjs/config": "0.6.3",
        "@nestjs/core": "7.6.18",
        "@nestjs/passport": "7.1.6",
        "@nestjs/platform-express": "7.6.18",
        "@nestjs/schedule": "1.0.1",
        "@nestjs/swagger": "4.8.2",
        "@nestjs/typeorm": "7.1.5",
        "auth0": "2.36.1",
        "binance": "2.0.29",
        "class-transformer": "0.4.0",
        "class-validator": "0.13.1",
        "date-fns": "2.23.0",
        "dotenv": "10.0.0",
        "jwks-rsa": "2.0.4",
        "nodemailer": "6.6.3",
        "passport": "0.4.1",
        "passport-jwt": "4.0.0",
        "pg": "8.7.1",
        "reflect-metadata": "0.1.13",
        "res": "0.4.0",
        "resource": "0.8.1",
        "rimraf": "3.0.2",
        "rxjs": "7.3.0",
        "swagger-ui-express": "4.1.6",
        "tslog": "3.2.1",
        "twitter-api-v2": "1.7.2",
        "typeorm": "0.2.37"
    },
    "devDependencies": {
        "@darraghor/eslint-plugin-nestjs-typed": "3.8.0",
        "@nestjs/schematics": "7.3.1",
        "@nestjs/testing": "7.6.18",
        "@types/auth0": "2.33.4",
        "@types/cron": "1.7.3",
        "@types/eslint": "7.28.2",
        "@types/express": "4.17.13",
        "@types/jest": "26.0.24",
        "@types/node": "15.12.4",
        "@types/nodemailer": "6.4.4",
        "@types/passport-jwt": "3.0.6",
        "@types/supertest": "2.0.11",
        "@typescript-eslint/eslint-plugin": "5.3.1",
        "@typescript-eslint/parser": "5.3.1",
        "eslint": "8.1.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-eslint-comments": "3.2.0",
        "eslint-plugin-import": "2.25.2",
        "eslint-plugin-jest": "25.2.2",
        "eslint-plugin-jsdoc": "37.0.0",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "eslint-plugin-promise": "5.1.1",
        "eslint-plugin-sonarjs": "0.10.0",
        "eslint-plugin-unicorn": "37.0.1",
        "jest": "27.3.1",
        "jest-create-mock-instance": "2.0.0",
        "jest-junit": "13.0.0",
        "prettier": "2.4.1",
        "supertest": "6.1.6",
        "ts-jest": "27.0.7",
        "ts-loader": "9.2.6",
        "ts-node": "10.4.0",
        "tsconfig-paths": "3.11.0",
        "typescript": "4.4.4"
    }
}
